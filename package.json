{
  "name": "turningpoint",
  "version": "0.2.0",
  "private": true,
  "workspaces": ["client", "server"],
  "engines": {
    "node": "16.x",
     "npm": "7.x"
  },
  "scripts": {
    "start": "npm run start -w server",
    "dev:all": "concurrently \"PORT=5005 nodemon ./bin/www\" \"PORT=3000 npm run start -w client\"",
    "dev:front": "PORT=3000 npm run start -w client",
    "dev:back": "PORT=5005 nodemon ./bin/www",
    "build": "npm run build -w client"
  },
  "dependencies": {
    "axios": "^0.21",
    "bcrypt": "^5",
    "cloudinary": "^1",
    "connect-mongo": "^4",
    "cookie-parser": "^1",
    "cors": "^2.8.5",
    "debug": "^2",
    "dotenv": "^10",
    "express-jwt": "^6.1.0",
    "express-session": "^1",
    "express": "^4",
    "hbs": "^4",
    "http-errors": "^1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.4",
    "morgan": "^1",
    "multer-storage-cloudinary": "^4",
    "multer": "^1",
    "nodemailer": "^6.6.5",
    "qs": "^6.10.1",
    "serve-favicon": "^2"
  },
  "devDependencies": {
    "debug": "^2",
    "nodemon": "^2",
    "concurrently": "^6"
  }
}